<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.BasicBankingSystem">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/sparks2"
        android:label="Sparks Bank App"
        android:roundIcon="@drawable/sparks2"
        android:supportsRtl="true"
        android:theme="@style/Theme.BasicBankingApp">
        <activity android:name="com.example.BasicBankingSystem.main.SendToUserList"></activity>
        <activity android:name="com.example.BasicBankingSystem.main.AllUsersList" />
        <activity android:name="com.example.BasicBankingSystem.main.TransactionHistory" />
        <activity android:name="com.example.BasicBankingSystem.main.UserData" />
        <activity android:name="com.example.BasicBankingSystem.main.HomeScreen" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>